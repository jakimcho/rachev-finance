<h1>Accounts#index</h1>

<h2> Accounts number <%= @accounts.length %> </h2>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>

  <% @accounts.each do |account| %>
    <tr>
      <td><%= account.name %></td>
      <td><%= account.balance %></td>
    </tr>
  <% end %>
  <%= link_to t("user.create_account"),  new_user_account_path(current_user) %>
</table>
