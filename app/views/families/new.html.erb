<h1>Families#new</h1>

<%= form_for :families, url: families_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :pass, "Password" %><br>
    <%= f.password_field :pass %>
  </p>


  <p>
    <%= f.label :confirm_password %><br>
    <%= f.password_field :confirm_password %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
